{{ block "timing" . }}
    <div class="grid grid-cols-2 even:border-t-2 even:border-slate-400 items-center justify-center">
        <div class="{{ if produces . }}text-green-500{{ else }}text-red-500{{end}}">
            {{ if eq .Duration 0 }} Running {{ else }} {{ humanizeDuration .Duration }} {{ end }}
        </div>
        <div>{{ formatTime .Start }}</div>
    </div>
{{ end }}
